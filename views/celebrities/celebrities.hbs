<h1>celebrities</h1>
<div class="row justify-content-center d-flex"></div>
{{#each celebArray}}
<div class="col-md-4 mb-3">
    <div class="card">

        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <p class="card-text"> Occupation:{{occupation}}</p>
            <p class="card-text">Catch Phrase:{{catchPhrase}}</p>
        </div>
    </div>
</div>
{{/each}}
</div>